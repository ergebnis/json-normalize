{
  "name": "ergebnis/json-normalize",
  "description": "Provides a console command for normalizing JSON documents.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com",
      "homepage": "https://localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/json-normalize",
  "support": {
    "issues": "https://github.com/ergebnis/json-normalize/issues",
    "source": "https://github.com/ergebnis/json-normalize",
    "security": "https://github.com/ergebnis/json-normalize/blob/main/.github/SECURITY.md"
  },
  "require": {
    "php": "~8.1.0 || ~8.2.0 || ~8.3.0",
    "ergebnis/json-normalizer": "^2.2.0",
    "ergebnis/json-printer": "^3.4.0",
    "ergebnis/json-schema-validator": "^2.0.0",
    "localheinz/diff": "^1.1.1",
    "symfony/console": "^5.4.1 || ^6.0.16"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.23.0",
    "ergebnis/data-provider": "^3.6.0",
    "ergebnis/license": "^2.7.0",
    "ergebnis/php-cs-fixer-config": "~6.60.0",
    "ergebnis/phpstan-rules": "^2.13.1",
    "ergebnis/phpunit-slow-test-detector": "^2.22.2",
    "ergebnis/rector-rules": "^1.9.0",
    "fakerphp/faker": "^1.24.1",
    "infection/infection": "~0.27.11",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^2.1.39",
    "phpstan/phpstan-deprecation-rules": "^2.0.4",
    "phpstan/phpstan-phpunit": "^2.0.16",
    "phpstan/phpstan-strict-rules": "^2.0.10",
    "phpunit/phpunit": "^10.5.62",
    "rector/rector": "^2.3.8"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Json\\Normalize\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Json\\Normalize\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "audit": {
      "abandoned": "report"
    },
    "platform": {
      "php": "8.1.26"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
