{
  "name": "ergebnis/json-normalize",
  "description": "Provides a console command for normalizing JSON documents.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/json-normalize",
  "support": {
    "issues": "https://github.com/ergebnis/json-normalize/issues",
    "source": "https://github.com/ergebnis/json-normalize"
  },
  "require": {
    "php": "~8.0.0 || ~8.1.0 || ~8.2.0",
    "ergebnis/json-normalizer": "^2.2.0",
    "ergebnis/json-printer": "^3.3.0",
    "ergebnis/json-schema-validator": "^2.0.0",
    "localheinz/diff": "^1.1.1",
    "symfony/console": "^5.4.1 || ^6.0.16"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.23.0",
    "ergebnis/data-provider": "^1.3.0",
    "ergebnis/license": "^2.1.0",
    "ergebnis/php-cs-fixer-config": "^5.2.0",
    "fakerphp/faker": "^1.21.0",
    "infection/infection": "~0.26.16",
    "phpunit/phpunit": "^9.5.27",
    "psalm/plugin-phpunit": "~0.18.4",
    "vimeo/psalm": "^5.4.0"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Json\\Normalize\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Json\\Normalize\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true
    },
    "platform": {
      "php": "8.0.25"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
